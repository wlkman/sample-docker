####searchIP#########################################################
docker build -f dockerFiles/searchIP -t myip .

docker run -it myip


####onbuild#########################################################
docker build -f dockerFiles/searchIP -t fatherimage .

docker run -it myip


####myTomcat#########################################################
docker build -f dockerFiles/myTomcat -t mytomcat .

docker run -it -p 9999:8080 mytomcat 

